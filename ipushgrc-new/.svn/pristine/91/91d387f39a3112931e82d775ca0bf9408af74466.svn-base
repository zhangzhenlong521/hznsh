	<!-- 分析以上2个数据, 对积分累计到达要处理的分值的员工进行处理
	查询的SQL:
 需要处理的违规积分记录, 条件: 1.没有处理过的, 2. 已达到要处理的分值
select * from (
select username, scorenow, score as scorenext, punish from (select username, sum(scorelost)as scorenow from cmp_score_lost where scoredate like '2011%' group by username)a, cmp_score_rule b
where a.scorenow >= b.score) a
left join (select username, max(scoretotal)as lastscore from cmp_score_punish where punishdate like '2011%' group by username) b
on a.username = b.username
where (b.lastscore is null or a.scorenext > b.lastscore)
	  -->
	<table name="cmp_score_punish" pkname="id" descr="违规惩罚处理">
        <columns>
            <col name="id"                      type="decimal"      length="22"     descr="主键" />
            <col name="userid"                  type="decimal"      length="22"     descr="违规人员ID" />
            <col name="username"                type="varchar"      length="30"     descr="违规人员" />
            <col name="deptid"                  type="decimal"      length="22"     descr="违规人员部门ID" />
            <col name="deptname"                type="varchar"      length="30"     descr="违规人员部门" />
            <col name="postname"                type="varchar"      length="30"     descr="违规人员岗位" />
			<col name="scoretotal"    			type="decimal"      length="3,1"      descr="分值"     remark="被处理时的积分"/>			
			<col name="punish"                  type="varchar"      length="50"     descr="惩罚"     remark="生成时带过来"/>
			<col name="punishdate"              type="char"         length="10"     descr="惩罚时间"  remark="处理时选择"/>
			<col name="remark"                  type="varchar"      length="500"    descr="备注"     remark="处理时填入"/>
            <col name="state"        		    type="varchar"      length="30"     descr="状态"    remart="未处理/已处理" />			
			<col name="createdate"   			type="char"         length="10"     descr="创建时间" />
            <col name="creater"     		    type="varchar"      length="30"     descr="创建人" />	
			<col name="lastdate"     		    type="varchar"      length="10"     descr="最后扣分时间" />            		         
        </columns>
        <indexs>
        	<index name="in_cmp_score_punish_1" cols="userid" descr="违规人员" />
        </indexs>
    </table>   
