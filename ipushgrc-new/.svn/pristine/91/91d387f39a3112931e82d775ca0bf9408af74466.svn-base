	<!-- ******************************* 【流程与风险,15张表,3个工作流,9个功能点】********************************** -->
	<table name="cmp_cmpfile" pkname="id" descr="流程/流程文件"   remark="核心对象表之一!!!">
	<columns>
		<col name="id"                    type="decimal"        length="22"       descr="主键" />
		<col name="cmpfilename"           type="varchar"        length="100"      descr="文件名称"  remark="以前还有文件编码,结果因为大多数客户的管理现状是编不了编码的,所以反受其乱!所以去掉"/>
		<col name="cmpfiletype"           type="varchar"        length="30"       descr="文件分类" />
		<col name="filestate"             type="varchar"        length="30"       descr="文件状态"    remark="用于决定某个文件是否有效!! 比如:废止,发布" />
		<col name="publishdate"           type="char"           length="10"       descr="发布日期" />
		<col name="versionno"             type="decimal"        length="4,2"      descr="版本号" />
		<col name="cmpfilecode"           type="varchar"        length="100"      descr="文件编码" />
		<col name="cmpfilesymbol"         type="varchar"        length="100"      descr="文件文号" />
		
		
		<col name="blcorpid"              type="decimal"        length="22"         descr="主管/所属部门id" />
		<col name="blcorpname"            type="varchar"        length="100"        descr="主管/所属部门名称" />
		<col name="bsactid"               type="decimal"        length="22"         descr="业务活动分类" />
		<col name="bsactname"             type="varchar"        length="100"        descr="业务活动分类名称" />
		<col name="productid"             type="varchar"        length="200"        descr="对应的产品id"  remark="一个流程文件对应多个产品，一个产品可对应多个流程文件"  />
		<col name="productname"           type="varchar"        length="1000"       descr="对应的产品名称" />
		<col name="ictypeid"              type="decimal"        length="22"         descr="内控五要素分类" />
		<col name="ictypename"            type="varchar"        length="100"        descr="内控五要素分类名称" />
		<col name="reffile"               type="varchar"        length="300"        descr="正文" />
		<col name="filelevel"             type="varchar"        length="30"         descr="文件级别" />
		<col name="checkcount"            type="decimal"        length="3"          descr="每季度检查次数" />
		
		<col name="item_target"           type="varchar"        length="2000"       descr="流程10条目_目的" />
		<col name="item_userarea"         type="varchar"        length="2000"       descr="流程10条目_适用范围" />
		<col name="item_keywords"         type="varchar"        length="2000"       descr="流程10条目_关键字缩写" />
		<col name="item_duty"             type="varchar"        length="2000"       descr="流程10条目_职责与权限" />
		<col name="item_bsprcp"           type="varchar"        length="2000"       descr="流程10条目_基础原则" />
		<col name="item_addenda"          type="varchar"        length="3000"        descr="流程10条目_附件" />
		<col name="item_formids"          type="varchar"        length="200"        descr="流程10条目_表单" />
		<col name="item_remark"           type="varchar"        length="2000"       descr="流程10条目_备注" />
		<col name="evalcount"             type="decimal"        length="10"         descr="评价次数" />
		<col name="billtype"              type="varchar"        length="50"         descr="单据类型" />
		<col name="busitype"              type="varchar"        length="50"         descr="业务类型" />
		<col name="wfprinstanceid"        type="decimal"        length="22"         descr="流程实例id"    remark="走工作流时使用" />
		<col name="createuser"            type="decimal"        length="22"         descr="创建人" />
		<col name="riskstate"             type="varchar"        length="20"         descr="风险状态"  remark="风险状态有两种：编辑中；有效。风险识别评估时如果风险状态为编辑中则可以结束编辑，此时状态为有效，流程文件才可以发布"  />		
     	<col name="filetype"              type="varchar"        length="20"         descr="文件类型"  remark="分为两种：流程文件和内控文件" />
     	<col name="manageobject"          type="decimal"        length="22"         descr="管理对象id"    remark="内控产品中管理对象icam_manage_object.id" />
	</columns>
	<indexs>
		<index name="in_cmp_cmpfile_1"    cols="cmpfilename"       descr="流程文件名称"  />
		<index name="in_cmp_cmpfile_2"    cols="blcorpid"          descr="所属机构id"    />
		<index name="in_cmp_cmpfile_3"    cols="bsactid"           descr="所属业务"      />
	</indexs>
    </table>
	<table name="cmpfile_eval" pkname="id" descr="流程文件评价">
