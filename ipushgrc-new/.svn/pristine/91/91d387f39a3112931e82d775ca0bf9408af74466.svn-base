<?xml version="1.0" encoding="GBK"?>
<!-- 标准产品的 -->
<root>
    
    <table name="my_favorites" pkname="id" descr="我的收藏夹">
        <columns>
            <col name="id"              type="decimal"      length="22"     descr="主键" />
            <col name="itemtype"        type="varchar"      length="30"     descr="条目类型" />
            <col name="itemname"        type="varchar"      length="100"    descr="条目名称" />
            <col name="templetname"     type="varchar"      length="100"    descr="模板名称" />
            <col name="itemid"          type="varchar"      length="30"     descr="条目ID" />
            <col name="type"            type="char"         length="1"      descr="参数类型" reamrk="0:原有默认机制，根据类打开.1：根据菜单ID打开。2：根据模板编码" />
            <col name="classpath"       type="varchar"      length="100"    descr="加载类" />            
            <col name="createdate"      type="char"         length="10"     descr="创建时间" />
            <col name="creater"         type="varchar"      length="30"     descr="创建人" />
            <col name="deptid"          type="decimal"      length="22"     descr="创建机构" />
            <col name="isfolder"        type="char"         length="1"      descr="是否是文件夹" />
            <col name="parentid"        type="decimal"      length="22"     descr="父亲节点" />
            <col name="seq"       	 	type="decimal"      length="22"     descr="排序" />
        </columns>
        <indexs>
            <index name="in_my_favorites_1" cols="creater" descr="创建人" />
            <index name="in_my_favorites_2" cols="parentid" descr="父亲节点" />
        </indexs>
    </table>
        
	<!-- ******************************* 【法律法规模块,4张表】 ********************************** -->
	<table name="law_law" pkname="id" descr="法律主表">
		<columns>
			<col name="id"              type="decimal"      length="22"     descr="主键" />
			<col name="lawname"         type="varchar"      length="400"    descr="法律名称" />
			<col name="lawtype"         type="varchar"      length="30"     descr="法规类型" />
			<col name="issuecorp"       type="varchar"      length="200"     descr="发布机构,对应于bsd_lawissuecorp" />
			<col name="usearea"         type="varchar"      length="50"     descr="适用区域" />
			<col name="issue_date"      type="char"         length="10"     descr="发布日期" />
			<col name="state"           type="varchar"      length="30"     descr="状态" />
			<col name="isnation"        type="char"         length="1"      descr="是否国际" />
			<col name="envername"       type="varchar"      length="200"    descr="英文译名" />
			<col name="dispatch_code"   type="varchar"      length="200"    descr="发文字号" />
			<col name="authorize_dept"  type="varchar"      length="50"     descr="批准部门" />
			<col name="authorize_date"  type="char"         length="10"     descr="批准日期" />
			<col name="implement_date"  type="char"         length="10"     descr="实施日期" />
			<col name="creatername"     type="varchar"      length="30"     descr="记录创建人" />
			<col name="createtime"      type="char"         length="19"     descr="记录创建时间" />
			<col name="remark"          type="varchar"      length="4000"   descr="备注" />
			<col name="textfile"        type="varchar"      length="200"    descr="正文" />
			<col name="attachfile"      type="varchar"      length="1000"   descr="附件" />
		    <col name="reflaw"          type="varchar"      length="1000"    descr="修订的法规" />
			<col name="reflaw2"      	type="varchar"      length="1000"    descr="废止的法规" />
		</columns>
		<indexs>
			<index name="in_law_law_1" cols="lawname" descr="法律名称" />
		</indexs>
	</table>
	
	<table name="law_law_importlog" pkname="id" descr="法规导入日志表">
      <columns>
            <col name="id"                   type="decimal"      length="22"     descr="主键" />
            <col name="descr"                type="varchar"      length="1000"   descr="本次修改内容" />
            <col name="deptid"               type="decimal"      length="22"     descr="操作部门"   />
            <col name="deptname"           	 type="varchar"      length="100"    descr="操作部门"   />
            <col name="userid"           	 type="decimal"      length="22"     descr="操作人"   />
            <col name="username"           	 type="varchar"      length="50"     descr="操作人"   />
            <col name="createdate"           type="varchar"      length="10"     descr="操作日期" />	
     </columns>
    </table>

	<table name="law_law_item" pkname="id" descr="法规子表" remark="法律是解析的,所以每一条款是存储在数据库中的一条记录">
		<columns>
			<col name="id"               type="decimal"    length="30"      descr="主键" />
			<col name="lawid"            type="decimal"    length="30"      descr="外规id" />
			<col name="itemtitle"        type="varchar"    length="400"     descr="法律条款标题" />
			<col name="itemcontent"      type="varchar"    length="4000"    descr="法律条款内容" />
			<col name="parentid"         type="decimal"    length="22"      descr="父亲记录的主键" />
			<col name="linkcode"         type="varchar"    length="60"      descr="关联码,以后考虑多使用该机制,因为parentid没有这个方便" />
			<col name="seq"              type="decimal"    length="22"      descr="排序字段" />
			<col name="attachfile"       type="varchar"    length="1000"    descr="附件" />
			<col name="state"       	 type="varchar"    length="1"    	descr="是否失效" reamrk="取值：Y;N; 如果为N，则链接查看时该条灰色显示"/>
		</columns>
		<indexs>
			<index name="in_law_law_item_1" cols="lawid" descr="外规名称" />
		</indexs>
	</table>

	<table name="law_mapping" pkname="id" descr="法规映射表" remark="业务活动与外规映射的,即以前一直提的内外规映射">
		<columns>
			<col name="id"              type="decimal"     length="30"      descr="主键" />
			<col name="bsactid"         type="decimal"     length="30"      descr="业务活动主键" />
			<col name="lawid"           type="decimal"     length="30"      descr="法律主键,对应于law_law.id,用于点击链接查看外规" />
			<col name="lawname"         type="varchar"     length="100"     descr="法律名称" />
			<col name="lawitemid"       type="varchar"     length="30"      descr="法律条文id" />
			<col name="lawitemtitle"    type="varchar"     length="100"     descr="法律条文标题" />
			<col name="lawitemcontent"  type="varchar"     length="4000"    descr="法律条文内容" />
		</columns>
		<indexs>
			<index name="in_law_mapping_1" cols="bsactid" descr="外规名称" />
		</indexs>
	</table>

	<table name="law_exposit" pkname="id" descr="法规解读">
		<columns>
			<col name="id"              type="decimal"      length="22"       descr="主键" />
			<col name="lawid"           type="varchar"      length="30"       descr="法律id" />
			<col name="lawname"         type="varchar"      length="100"      descr="法律名称" />
			<col name="lawitemid"       type="varchar"      length="30"       descr="法律条文id" />
			<col name="lawitemtitle"    type="varchar"      length="100"      descr="法律条文标题" />
			<col name="lawitemcontent"  type="varchar"      length="4000"     descr="法律条文内容" />
			<col name="expositmsg"      type="varchar"      length="4000"     descr="解读内容" />
			<col name="exposituserid"   type="decimal"      length="22"       descr="解读人员" />
			<col name="expositusername" type="varchar"      length="30"       descr="解读人员名称" />
			<col name="exposittime" 	type="char"         length="10"       descr="解读时间" />
		</columns>
		<indexs>
			<index name="in_law_exposit_1" cols="lawname" descr="外规名称" />
			<index name="in_law_exposit_2" cols="lawid" descr="法律id" />
		</indexs>
	</table>
	<table name="law_bsact" pkname="id" descr="业务活动" remark="法律法规目录树">
		<columns>
			<col name="id" type="decimal" length="22" descr="主键" />
			<col name="name" type="varchar" length="200" descr="名称" />
			<col name="parentid" type="decimal" length="22" descr="父记录id" />
			<col name="seq" type="decimal" length="22" descr="排序字段" />
			<col name="linkcode" type="varchar" length="60" descr="关联字段" remark="以后想更多采用linkcode关联机制" />
		</columns>
		<indexs></indexs>
	</table>
	<!-- ******************************* 【内部制度模块,7张表】********************************** -->
      <table name="rule_rule" pkname="id" descr="内规主表" remark="核心对象表之一!!!">
		<columns>
			<col name="id"                type="decimal"         length="22"     descr="主键" />
			<col name="rulecode"          type="varchar"         length="50"     descr="制度文号" />
			<col name="rulename"          type="varchar"         length="150"    descr="制度名称" />
			<col name="ruletype"          type="varchar"         length="30"     descr="业务分类,比如:管理办法,操作规程" />
			<col name="publishdate"       type="char"            length="10"     descr="发布日期" />
			<col name="busitype"          type="varchar"         length="200"     descr="属于什么业务类型" />
			<col name="busitype_name"     type="varchar"         length="100"    descr="业务分类名称,为了统计方便与加载性能" />
			<col name="ictype"            type="decimal"         length="22"     descr="内控分类" />
			<col name="ictype_name"       type="varchar"         length="100"    descr="内控分类名称,为了统计方便与加载性能" />
			<col name="producttype"       type="decimal"         length="22"     descr="产品分类" />
			<col name="producttype_name"  type="varchar"         length="100"    descr="产品分类名称" />
			<col name="blcorp"            type="decimal"         length="22"     descr="主管部门" />
			<col name="blcorp_name"       type="varchar"         length="100"    descr="主管部门名称" />
			<col name="createuser"        type="varchar"         length="200"    descr="创建人员" />
			<col name="sendtocorp"        type="varchar"         length="100"    descr="主送部门,多选参照" />
			<col name="sendtocorp_name"   type="varchar"         length="100"    descr="主送部门名称" />
			<col name="cctocorp"          type="varchar"         length="100"    descr="抄送部门" />
			<col name="cctocorp_name"     type="varchar"         length="100"    descr="抄送部门名称" />
			<col name="state"             type="varchar"         length="30"     descr="状态" />
			<col name="abolishdate"       type="char"            length="10"     descr="作废日期" />
			<col name="textfile"          type="varchar"         length="500"    descr="正文" />
			<col name="attachfile"        type="varchar"         length="3000"   descr="附件" />
			<col name="formids"           type="varchar"         length="500"    descr="表单" />
			<col name="reflaw"            type="varchar"         length="300"    descr="关联法律" />
			<col name="refrule"           type="varchar"         length="300"    descr="关联制度" />
			<col name="evalcount"         type="decimal"         length="10"     descr="评价次数" />
			<col name="rule_level"        type="varchar"         length="50"     descr="层级(总公司/分子公司)" />
            <col name="file_system"       type="varchar"         length="50"     descr="文件体系(ISO, HSE)" />
            <col name="createdate"        type="varchar"         length="10"     descr="创建日期" />			
		</columns>
		<indexs>
			<index name="in_rule_rule_1"   cols="rulecode"     descr="制度文号" />
			<index name="in_rule_rule_2"   cols="rulename"     descr="制度名称" />
			<index name="in_rule_rule_3"   cols="publishdate"  descr="发布日期" />
			<index name="in_rule_rule_4"   cols="blcorp"  descr="主管部门" />
		</indexs>
	</table>

	<table name="rule_rule_item" pkname="id" descr="内规子表">
		<columns>
			<col name="id"               type="decimal"           length="22"      descr="主键" />
			<col name="ruleid"           type="decimal"           length="30"      descr="制度id" />
			<col name="itemtitle"        type="varchar"           length="100"     descr="制度条款标题" />
			<col name="itemcontent"      type="varchar"           length="4000"    descr="制度条款内容" />
			<col name="parentid"         type="decimal"           length="22"      descr="父亲记录的主键" />
			<col name="linkcode"         type="varchar"           length="60"      descr="关联码,以后考虑多使用该机制,因为parentid没有这个方便" />
			<col name="seq"              type="decimal"           length="22"      descr="排序字段" />
			<col name="attachfile"       type="varchar"           length="300"     descr="附件" />
		</columns>
		<indexs>
			<index name="in_rule_rule_item_1"    cols="ruleid"      descr="外规id" />
		</indexs>
	</table>

	<table name="rule_eval" pkname="id" descr="内规评价">
		<columns>
			<col name="id"               type="decimal"          length="22"       descr="主键" />
			<col name="evaltype"         type="varchar"          length="4000"     descr="评价类型"    reamrk="比如:建议新增制度,修改某制度,废止某制度" />
			<col name="ruleid"           type="varchar"          length="30"       descr="制度id"      remark="如果是修改或删除制度,则有值,新增类型不需要!" />
			<col name="rulename"         type="varchar"          length="100"      descr="制度名称" />
			<col name="evaltitle"        type="varchar"          length="300"      descr="评价的标题" />
			<col name="evalmsg"          type="varchar"          length="2000"     descr="评价内容"    remark="多行文本框,以后可以使用多风格文本框" />
			<col name="evaldate"         type="char"             length="10"       descr="评价日期" />
			<col name="evaluserid"       type="decimal"          length="22"       descr="评价人员" />
			<col name="evalusername"     type="varchar"          length="30"       descr="评价人员名称" />
			<col name="attachfile"       type="varchar"          length="300"      descr="附件" />
		</columns>
		<indexs>
			<index name="in_rule_eval_1"          cols="ruleid"        descr="制度id" />
		</indexs>
	</table>
	
	<table name="rule_eval_b" pkname="id" descr="内规评价子表"  remark="即对某个评价回贴">
	   <columns>
			<col name="id"                 type="decimal"          length="22"       descr="主键" />
			<col name="rule_eval_id"       type="decimal"          length="22"       descr="评价主表"    reamrk="对应于rule_eval.id" />
			<col name="reverttype"         type="varchar"          length="10"       descr="回复类型"    remark="比如:顶,支持,不同意" />
			<col name="reverttitle"        type="varchar"          length="300"      descr="回复标题" />
			<col name="revertmsg"          type="varchar"          length="2000"     descr="回复内容"    remark="多行文本框,以后可以使用多风格文本框" />
			<col name="revertdate"         type="char"             length="10"       descr="评价日期" />
			<col name="revertuserid"       type="decimal"          length="22"       descr="评价人员" />
			<col name="revertusername"     type="varchar"          length="30"       descr="评价人员名称" />
			<col name="attachfile"         type="varchar"          length="300"      descr="附件" />
		</columns>
		<indexs>
			<index name="in_rule_eval_b_1"   cols="rule_eval_id"        descr="评价主表" />
			<index name="in_rule_eval_b_2"   cols="revertuserid"        descr="评价人员" />
		</indexs>
	</table>

	<table name="rule_relation" pkname="id" descr="制度自身关联表"   remark="即内规是多对多的自链接">
		<columns>
			<col name="id"               type="decimal"         length="22"         descr="主键" />
			<col name="ruleid1"          type="varchar"         length="22"         descr="制度1的主键" />
			<col name="ruleid2"          type="varchar"         length="22"         descr="制度2的主键" />
		</columns>
		<indexs>
			<index name="int_rule_relation_1" cols="ruleid1" descr="制度1的主键" />
			<index name="int_rule_relation_2" cols="ruleid2" descr="制度1的主键" />
		</indexs>
	</table>

	<table name="rule_apply_wf" pkname="id" descr="制度评审流程表"   remark="创建内规之申请流程,即通过走流程来创建内规,或者叫内规评审流程,招行与兴业都有这个,但兴业做的最复杂,还有子流程">
		<columns>
			<col name="id"               type="decimal"         length="22"         descr="主键" />
			<col name="rulecode"         type="varchar"         length="100"        descr="制度编码" />
			<col name="rulename"         type="varchar"         length="100"        descr="制度名称" />
			<col name="applyreason"      type="varchar"         length="500"        descr="申请原因" />
			<col name="creater"          type="decimal"         length="22"         descr="申请人" />
			<col name="creater_name"     type="varchar"         length="100"        descr="申请人名称" />
			<col name="createcorp"       type="decimal"         length="22"         descr="申请机构" />
			<col name="createcorp_name"  type="varchar"         length="100"        descr="申请机构名称" />
			<col name="createdate"       type="char"            length="10"         descr="申请日期" />
			<col name="billtype"              type="varchar"        length="50"        descr="单据类型" />
		    <col name="busitype"              type="varchar"        length="50"        descr="业务类型" />
		    <col name="wfprinstanceid"        type="decimal"        length="22"        descr="流程实例id"    remark="走工作流时使用" />
		</columns>
		<indexs></indexs>
	</table>

	<table name="rule_abolish_wf" pkname="id" descr="内规废止申请流程">
		<columns>
			<col name="id"               type="decimal"         length="22"         descr="主键" />
			<col name="ruleid"           type="decimal"         length="22"         descr="制度id"   remark="对应于制度的id,即对哪个制度进行废止!" />
			<col name="rulecode"         type="varchar"         length="100"        descr="制度编码" />
			<col name="rulename"         type="varchar"         length="100"        descr="制度名称" />
			<col name="abolishreason"    type="varchar"         length="500"        descr="废止原因" />
			<col name="creater"          type="decimal"         length="22"         descr="申请人" />
			<col name="creater_name"     type="varchar"         length="100"        descr="申请人名称" />
			<col name="createcorp"       type="decimal"         length="22"         descr="申请机构" />
			<col name="createcorp_name"  type="varchar"         length="100"        descr="申请机构名称" />
			<col name="createdate"       type="char"            length="10"         descr="申请日期" />
			
			<col name="billtype"              type="varchar"        length="50"        descr="单据类型" />
		    <col name="busitype"              type="varchar"        length="50"        descr="业务类型" />
		    <col name="wfprinstanceid"        type="decimal"        length="22"        descr="流程实例id"    remark="走工作流时使用" />
		</columns>
		<indexs></indexs>
	</table>
	
	<table name="rule_alarm" pkname="id" descr="制度预警" remark="">
		<columns>
			<col name="id"               type="decimal"          length="22"        descr="主键" />
			<col name="ruleid"           type="decimal"          length="22"        descr="主键" />
			<col name="rulecode"         type="varchar"          length="100"       descr="制度文号" />
			<col name="rulename"         type="varchar"          length="100"       descr="制度名称" />
			<col name="alarmreason"      type="varchar"          length="100"       descr="警告原因" />
			<col name="alarmsource"      type="varchar"          length="100"       descr="警告来源" />
			<col name="alarmsourcetab"   type="varchar"          length="100"       descr="警告来源表名" />
			<col name="alarmsourcepk"    type="varchar"          length="100"       descr="警告来源主键值" />
			<col name="alarmdate"   	 type="char"          	 length="10"        descr="预警时间" />
			<col name="state"   	     type="varchar"          length="20"        descr="状态" />
			<col name="alarmtargettab"   type="decimal"         length="22"         descr="警告修改表" />
			<col name="creator"     	 type="decimal"         length="22"         descr="警告来源人" />
			<col name="createdept"       type="decimal"         length="22"         descr="警告来源机构" />
			<col name="manager"  		 type="decimal"         length="22"         descr="警告处理人" />
			<col name="managedept"       type="decimal"         length="22"         descr="警告处理机构" />
			<col name="managedate"       type="char"            length="10"         descr="警告处理日期" />
		</columns>
		<indexs></indexs>
	</table>
	
